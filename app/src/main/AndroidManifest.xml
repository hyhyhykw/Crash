<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.hy.hotdog">

    <application
        android:name="com.hy.hotdog.CrashApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.hy.crash.CrashErrorActivity"
            android:theme="@style/CrashQQTheme"/>

        <activity
            android:name="com.hy.crash.CrashErrorLogListActivity"
            android:theme="@style/CrashQQTheme"/>

        <activity
            android:name="com.hy.crash.CrashLogActivity"
            android:theme="@style/CrashQQTheme"/>
    </application>

</manifest>